generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Category {

  idCategory     Int        @id @default(autoincrement())
  nameCategory String
  entryDate    DateTime
  Product        Product[]

}

model Product {

  idProduct     Int       @id @default(autoincrement())
  nameProduct String
  entryDate   DateTime
  modelProduct String
  idCategory    Int
  idState Int 
  state State @relation(fields: [idState], references: [idState])
  category      Category @relation(fields: [idCategory], references: [idCategory])
  
}

model State{
  
  idState Int @id @default(autoincrement())
  nameState String
  Product Product[]

}
